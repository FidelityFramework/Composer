[package]
name = "HelloWorldDirect"
version = "0.1.0"
description = "HelloWorld with direct module calls"
authors = ["SpeakEZ Technologies, Inc."]
license = "MIT"

[compilation]
target = "cpu"

[dependencies]
# Types and operations are FNCS intrinsics - no external library needed
platform = { path = "/home/hhh/repos/Fidelity.Platform/CPU/Linux/x86_64" }

[build]
sources = ["HelloWorld.fs"]
output = "helloworld"
output_kind = "console"
